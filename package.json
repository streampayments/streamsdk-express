{
  "name": "stream-sdk-express",
  "version": "1.0.0",
  "description": "Express.js adapter for Stream SDK - Declarative handlers for checkout and webhooks",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "stream",
    "streampay",
    "payment",
    "express",
    "adapter",
    "middleware",
    "checkout",
    "webhooks",
    "typescript",
    "saudi-arabia"
  ],
  "author": "StreamPay <support@streampay.sa>",
  "contributors": [
    "Ibtisam <ibtisam@streampay.sa>"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/streampayments/stream-sdk-express.git"
  },
  "bugs": {
    "url": "https://github.com/streampayments/stream-sdk-express/issues"
  },
  "homepage": "https://github.com/streampayments/stream-sdk-express#readme",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "express": "^4.18.0 || ^5.0.0"
  },
  "dependencies": {
    "stream-sdk": "file:../stream-sdk"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "express": "^4.18.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
